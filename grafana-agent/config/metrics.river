prometheus.scrape "container_metrics" {
  targets = [
    {"__address__" = "cadvisor:8080"},
  ]
  forward_to = [ prometheus.remote_write.mimir.receiver ]
}

prometheus.remote_write "mimir" {
  // external_labels = {
  //   cluster = "docker-compose",
  // }

  endpoint {
    url = "http://mimir:8080/api/v1/push"
  }
}